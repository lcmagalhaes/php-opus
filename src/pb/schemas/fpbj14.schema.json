{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Dados Previdenciais",
  "type": "object",
  "properties": {
    "dadosPrevidenciais": {
      "description": "Dados previdenciais",
      "type": "object",
      "minProperties": 1,
      "properties": {
        "planoPrevidencia": {
          "type": "array",
          "items": {
            "type": "object",
            "minProperties": 1,
            "properties": {
              "matriculas": {
                "description": "Matriculas da pessoa, de acordo com o sistema de origem. <b>Nota:</b> A vers√£o 1.0 trabalha somente com 'matriculaSistemaCentral'",
                "type": "object",
                "minProperties": 1,
                "properties": {
                  "matriculaSistemaCentral": {
                    "description": "Matricula da pessoa no Sistema Central (Opus/Openbase)",
                    "type": "object",
                    "minProperties": 1,
                    "properties": {
                      "numero": {
                        "type": "string",
                        "minLength": 1
                      },
                      "sequencial": {
                        "type": "string",
                        "minLength": 1
                      }
                    },
                    "required": [
                      "numero"
                    ]
                  }
                },
                "required": [
                  "matriculaSistemaCentral"
                ]
              },
              "statusBeneficiario": {
                "type": "string",
                "enum": [
                  "ATIVO",
                  "EM_SUSPENSAO",
                  "SUSPENSO",
                  "EM_DESLIGAMENTO",
                  "DESLIGADO"
                ]
              },
              "dataInscricao": {
                "type": "string",
                "minLength": 1,
                "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
              },
              "dataCancelamento": {
                "type": "string",
                "minLength": 1,
                "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
              },
              "dataInscricaoPeculio": {
                "type": "string",
                "minLength": 1,
                "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
              },
              "dataCancelamentoPeculio": {
                "type": "string",
                "minLength": 1,
                "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
              },
              "numeroInscricaoPrevidencia": {
                "type": "integer",
                "minLength": 1
              },
              "numeroInscricaoPeculio": {
                "type": "integer",
                "minLength": 1
              },
              "codigoBeneficio": {
                "type": "integer",
                "minLength": 1
              },
              "situacaoBeneficio": {
                "type": "integer",
                "minLength": 1
              },
              "tiposPeculio": {
                "type": "string",
                "enum": [
                  "O",
                  "A",
                  "B",
                  "C",
                  "D",
                  "E"
                ]
              },
              "condicaoPeculio": {
                "type": "string",
                "enum": [
                  "CANCELADO",
                  "ATIVO",
                  "MANTIDO",
                  "MANTIDO_PARCIAL",
                  "ASSISTIDO"
                ]
              },
              "baseSalarialPrevidencia": {
                "type": "number",
                "minLength": 1
              }
            },
            "required": [
              "statusBeneficiario",
              "dataInscricao"
            ]
          }
        }
      },
      "required": [
        "planoPrevidencia"
      ]
    }
  }
}
